{

	"name": "timeUp",
	"version": "1.0",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["*://www.netflix.com/*"],
			"js": ["content.js"]
		}
	],

	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "gymSmart"
	},
	"background" : {
		"scripts": ["background.js"]
	},

	"permissions": ["alarms", "notifications"]


} 